generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

// Define the destination model
model Destination {
  id          Int     @id @default(sequence())
  name        String @unique @db.String(255)
  images      Json
  description String
  distance    String
  travel      String
}

// Define the crew member model
model CrewMember {
  id     Int    @id @default(sequence())
  name   String @unique @db.String(255)
  images Json
  role   String
  bio    String
}

// Define the technology model
model Technology {
  id          Int    @id @default(sequence())
  name        String @unique @db.String(255)
  images      Json
  description String
}


